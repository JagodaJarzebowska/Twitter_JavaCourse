<?xml version="1.0" encoding="UTF-8" ?>

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>MessageServlet</servlet-name>
    <servlet-class>com.sda.twitter.servlets.MessageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MessageServlet</servlet-name>
    <url-pattern>/message</url-pattern>
  </servlet-mapping>


  <filter>
    <filter-name>MyFilter</filter-name>
    <filter-class>com.sda.twitter.filters.MyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>MyFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>


  <filter>
    <filter-name>TestFilter</filter-name>
    <filter-class>com.sda.twitter.filters.TestFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>TestFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

  <filter>
    <filter-name>TestFilter2</filter-name>
    <filter-class>com.sda.twitter.filters.TestFilter2</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>TestFilter2</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

  <filter>
    <filter-name>TestFilter3</filter-name>
    <filter-class>com.sda.twitter.filters.TestFilter3</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>TestFilter3</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

</web-app>